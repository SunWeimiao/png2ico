   "source": [
    "# Convert .png to .ico using PIL\n",
    "def pic2ico(path):\n",
    "    img = Image.open(path)\n",
    "    filename = img.filename.split('.')[-2] + \".ico\"\n",
    "    img.save(filename)"
   ]
  },
